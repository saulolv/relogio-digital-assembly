                                 ; div10
                                 ; Divide valor de temp1 por 10
                                 ; Retorno: temp1 = dezena (quociente), temp2 = unidade (resto)
                                 ; ============================
                                 div10:
000422 2711                          clr temp2                       ; Zera temp2 (ser o quociente)
                                 div10_loop:
000423 300a                          cpi temp1, 10                   ; Verifica se temp1 ainda  maior ou igual a 10
000424 f018                          brlo div10_end                  ; Se for menor que 10, fim da diviso
000425 500a                          subi temp1, 10                  ; Subtrai 10 de temp1
000426 9513                          inc temp2                       ; Incrementa o quociente
000427 cffb                          rjmp div10_loop                 ; Repete at temp1 < 10
                                 
                                 div10_end:
000428 930f                          push temp1                      ; Salva o resto (que ainda est em temp1)
000429 2f01                          mov temp1, temp2                ; Coloca o quociente (dezena) em temp1
00042a 911f                          pop temp2                       ; Recupera o resto (unidade) para temp2
